<h1>States</h1>

<% states=["Wyoming", "Colorado", "Utah", "Arizona", "New Mexico"] %>

<ul>
  <% states.each do|state| %>
  <li><h2> <%= state %> </h2>
</li>
  <% end %>
</ul>


<h1>Cities</h1>
<% cities=["Cheyenne", "Boulder", "Salt Lake City", "Tucson", "Taos"] %>

<ul>
  <% cities.each do|city| %>
  <li><h2> <%= city %> </h2>
</li>
  <% end %>
</ul>
